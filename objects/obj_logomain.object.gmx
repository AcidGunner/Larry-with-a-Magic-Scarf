          <argument>
            <kind>1</kind>
            <string>draw_set_alpha(alpha_val);
draw_set_color(c_white);
draw_set_halign(fa_center);
draw_set_valign(fa_top);

if (phase == 0) {
    //background
    draw_clear(c_black);
    
    // Overlay fading background
    var col_bg_light = make_color_rgb(180,180,180); // light gray
    var col_bg_dark  = make_color_rgb(0,0,0);       // black
    
    // Interpolate between dark and light
    var final_col = merge_color(col_bg_dark, col_bg_light, alpha_test);
    
    // Fullscreen rectangle with that color
    draw_set_color(final_col);
    draw_rectangle(0, 0, room_width, room_height, false);
    draw_set_color(c_white);
    
    // Colors
    var col_top    = make_color_rgb(120, 255, 120);
    var col_bottom = make_color_rgb(0, 120, 0);
    
    draw_circle_colour(cx, cy, 62, col_top, col_bottom, false);
    draw_circle_colour(cx, cy, 62, c_black, c_black, true);
    
    var radius = 30;
    var height = 40;
    var size = 30;
    var base_y_scale = 0.5;
    logo_rot += 1; // rotation speed
    if (logo_rot &gt;= 360) logo_rot -= 360;
    
    // Rotate base vertices around center
    var angle_rad = degtorad(logo_rot);
    
    // Base vertices relative to center// Store original positions
    var x1 = cx - size; var y1 = cy + size;
    var x2 = cx + size; var y2 = cy + size;
    var x3 = cx + size; var y3 = cy - size;
    var x4 = cx - size; var y4 = cy - size;
    
    var ox1 = x1; var oy1 = y1;
    var ox2 = x2; var oy2 = y2;
    var ox3 = x3; var oy3 = y3;
    var ox4 = x4; var oy4 = y4;
    
    // Apply rotation around center
    var x1 = cx + (ox1-cx)*cos(angle_rad) - (oy1-cy)*sin(angle_rad);
    var y1 = cy + (ox1-cx)*sin(angle_rad) + (oy1-cy)*cos(angle_rad);
    
    var x2 = cx + (ox2-cx)*cos(angle_rad) - (oy2-cy)*sin(angle_rad);
    var y2 = cy + (ox2-cx)*sin(angle_rad) + (oy2-cy)*cos(angle_rad);
    
    var x3 = cx + (ox3-cx)*cos(angle_rad) - (oy3-cy)*sin(angle_rad);
    var y3 = cy + (ox3-cx)*sin(angle_rad) + (oy3-cy)*cos(angle_rad);
    
    var x4 = cx + (ox4-cx)*cos(angle_rad) - (oy4-cy)*sin(angle_rad);
    var y4 = cy + (ox4-cx)*sin(angle_rad) + (oy4-cy)*cos(angle_rad);
    
    // Top vertex (above base)
    var xt = cx;
    var yt = cy - height;
    
    // Draw 4 side triangles (faces)
    draw_triangle_colour(x1, y1, x2, y2, xt, yt, col_bottom, col_bottom, col_top, false);
    draw_triangle_colour(x2, y2, x3, y3, xt, yt, col_bottom, col_bottom, col_top, false);
    draw_triangle_colour(x3, y3, x4, y4, xt, yt, col_bottom, col_bottom, col_top, false);
    draw_triangle_colour(x4, y4, x1, y1, xt, yt, col_bottom, col_bottom, col_top, false);
    
    draw_triangle_colour(x1, y1, x2, y2, xt, yt, c_black, c_black, c_black, true);
    draw_triangle_colour(x2, y2, x3, y3, xt, yt, c_black, c_black, c_black, true);
    draw_triangle_colour(x3, y3, x4, y4, xt, yt, c_black, c_black, c_black, true);
    draw_triangle_colour(x4, y4, x1, y1, xt, yt, c_black, c_black, c_black, true);
    // --- Text with alpha ---
    draw_set_color(c_black);
    draw_set_font(logofont);
    drawTextA(cx, cy + sizez + 40, "AcidGun Studios",alpha_val);
}
else if (phase == 1) {
    // PlayStation logo
    logo_rot2 += 0.5; // rotation speed
    if (logo_rot2 &gt;= 360) logo_rot2 -= 360;
    var angle_rad = degtorad(logo_rot2);
    
    var sc_x = sin(angle_rad)/2;
    
    draw_sprite_ext(spr_logo,0,cx,cy,sc_x,0.5,0,c_white,alpha_val);
    draw_set_font(logofont2);
    drawTextA(cx, cy + sizez + 40, "Made with Gamemaker 1.4",alpha_val);
}

//reset
draw_set_halign(fa_left);
draw_set_valign(fa_top);
draw_set_alpha(1);

//TEST
if show_time=1
{
    draw_set_font(logofont2);
    drawTextA(0,0,time,1)
}
</string>
